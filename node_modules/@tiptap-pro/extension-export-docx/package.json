{
  "name": "@tiptap-pro/extension-export-docx",
  "description": "Export your Tiptap document to a .docx file",
  "version": "1.0.0-beta.10.3",
  "homepage": "https://tiptap.dev/docs/editor/extensions/functionality/export",
  "keywords": [
    "tiptap",
    "tiptap export",
    "tiptap export docx",
    "tiptap extension"
  ],
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "dependencies": {
    "docx": "^9.5.1",
    "image-size": "^2.0.1"
  },
  "devDependencies": {
    "@tiptap/core": "^3.0.7",
    "@tiptap/pm": "^3.0.7"
  },
  "peerDependencies": {
    "@tiptap/core": "^3.0.7",
    "@tiptap/pm": "^3.0.7"
  },
  "publishConfig": {
    "registry": "https://registry.tiptap.dev"
  },
  "scripts": {
    "build": "tsup",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  }
}