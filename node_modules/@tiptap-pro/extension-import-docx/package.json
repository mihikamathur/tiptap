{
  "name": "@tiptap-pro/extension-import-docx",
  "description": "import docx extension for tiptap",
  "version": "1.0.0-beta.5",
  "homepage": "https://tiptap.dev/docs/editor/extensions/functionality/import",
  "keywords": [
    "tiptap",
    "tiptap extension"
  ],
  "type": "module",
  "license": "SEE LICENSE IN LICENSE.md",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ueberdosis"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "devDependencies": {
    "@tiptap/core": "^3.0.7",
    "@tiptap/pm": "^3.0.7",
    "@types/node": "^22.13.1"
  },
  "peerDependencies": {
    "@tiptap/core": "^3.0.7",
    "@tiptap/pm": "^3.0.7"
  },
  "publishConfig": {
    "registry": "https://registry.tiptap.dev"
  },
  "scripts": {
    "build": "tsup",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  }
}